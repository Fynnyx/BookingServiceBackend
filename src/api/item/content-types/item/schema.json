{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "richtext",
      "required": true
    },
    "Price": {
      "type": "decimal",
      "default": 0,
      "required": true,
      "min": 0
    },
    "Thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "item"
    },
    "BedCount": {
      "type": "integer",
      "min": 0,
      "required": true
    },
    "hasNetwork": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "hasGarden": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "hasPool": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "hasShower": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "hasBathtub": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "hasKitchen": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "Address": {
      "type": "customField",
      "customField": "plugin::google-maps.location-picker",
      "required": true
    },
    "Contact": {
      "type": "component",
      "repeatable": false,
      "component": "meta.contact",
      "required": true
    }
  }
}
